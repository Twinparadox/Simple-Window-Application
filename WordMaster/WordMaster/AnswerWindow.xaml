<Window x:Class="WordMaster.AnswerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WordMaster"
        mc:Ignorable="d"
        Title="WordMaster - 문제" Height="140" Width="600" Loaded="Window_Loaded" ResizeMode="NoResize" Topmost="True" WindowStartupLocation="CenterScreen" WindowStyle="None">
    <Grid Width="600" Height="140">
        <Button x:Name="ButtonSubmit" Content="Button" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="515,110,0,0" Click="ButtonSubmit_Click"/>
        <Button x:Name="ButtonStop" Content="Button" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="10,110,0,0"/>
        <TextBox x:Name="TextBoxEnglish" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="580" Margin="10,10,0,0" IsEnabled="False"/>
        <TextBox x:Name="TextBoxKorean" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="580" Margin="10,53,0,0"/>

    </Grid>
</Window>
